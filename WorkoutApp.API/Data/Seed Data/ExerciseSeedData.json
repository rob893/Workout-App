    {
        "Id": 5,
        "Name": "Squat",
        "PrimaryMuscle": {
            "Id": 1,
            "Name": "Quadriceps"
        },
        "SecondaryMuscle": {
            "Id": 4,
            "Name": "Gluteus"
        },
        "ExerciseSteps": [
            {
                "ExerciseStepNumber": 1,
                "Description": "Begin with the barbell supported on top of the traps. The chest should be up and the head facing forward. Adopt a hip-width stance with the feet turned out as needed."
            },
            {
                "ExerciseStepNumber": 2,
                "Description": "Descend by flexing the knees, refraining from moving the hips back as much as possible. This requires that the knees travel forward. Ensure that they stay align with the feet. The goal is to keep the torso as upright as possible."
            },
            {
                "ExerciseStepNumber": 3,
                "Description": "Continue all the way down, keeping the weight on the front of the heel. At the moment the upper legs contact the lower legs reverse the motion, driving the weight upward."
            }
        ],
